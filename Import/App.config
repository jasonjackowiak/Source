      <RuleFile file="..\..\..\..\HSA Input\extract_rules_common.txt" />
      <JobCardLanguageFile file="..\..\..\..\HSA Input\UNKNOWN" />
      <CobolFile file="..\..\..\..\HSA Input\UNKNOWN" />
